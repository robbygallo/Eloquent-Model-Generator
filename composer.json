{
    "name": "robbygallo/eloquent-model-generator",
    "description": "Auto-generates all Eloquent models from the database in a Laravel 5 in my project.",
    "keywords": ["laravel", "model", "eloquent", "artisan"],
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Roberto Galli",
            "email": "r.galli@hotmail.it"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": ">=4.1",
        "xethron/migrations-generator": "*"
    },
    "repositories": [
        {
            "type": "git",
            "url": "git@github.com:jamisonvalenta/Laravel-4-Generators.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "Robbygallo\\EloquentModelGenerator\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.0.0",
        "mockery/mockery": ">=0.9.0",
        "illuminate/cache": ">=4.1.0",
        "illuminate/console": ">=4.1.0"
    }
}
